   "source": [
    "### Singleton Detection with No Memory"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "3"
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def find_singleton(tmp):\n",
    "    ret = tmp[0]\n",
    "    for val in tmp[1:]:\n",
    "        ret ^= val\n",
    "    return ret\n",
    "\n",
    "tmp = [1, 2, 3, 2, 1]\n",
    "find_singleton(tmp)"
   ]
  },
