   "source": [
    "def double_palindrome(i):\n",
    "    return str(i) == str(i)[::-1] and bin(i)[2:] == bin(i)[2:][::-1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "double_palindomes = []\n",
    "for i in range(1000000):\n",
    "    if double_palindrome(i):\n",
    "        double_palindomes.append(i)"
   ]
  },
