   "source": [
    "def is_palindrome(n):\n",
    "    return str(n) == str(n)[::-1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "palindromes = []\n",
    "for i in range(100, 1000):\n",
    "    for j in range(100, 1000):\n",
    "        prod = i * j\n",
    "        if is_palindrome(prod):\n",
    "            palindromes.append(prod)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
